  # Get vocabulary word translation in interface language
  def get_word_translation(learning_word, learning_language, interface_language) do
    VocabularyTranslator.get_word_translation(learning_word, learning_language, interface_language)
  end


  def current_vocabulary_item(content, index) do
    Enum.at(content.vocabulary, index)
  end

  # Get the meaning/translation of an example sentence in the interface language
  def get_example_meaning(original_word, learning_language, interface_language) do
    # If learning and interface languages are the same, don't show translation
    if learning_language == interface_language do
      nil
    else
      get_example_translation(original_word, learning_language, interface_language)
    end
  end

  # This function is now handled by ExampleTranslator module  
  defp get_example_translation(word, _learning_language, interface_language) do
    ExampleTranslator.get_example_translation(word, interface_language)
  end
