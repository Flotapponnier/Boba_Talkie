  # Adapt tutorial content based on the learning language selected
  defp adapt_content_to_learning_language(content, learning_language) do
    case learning_language do
      "en" -> content  # English is the default
      "fr" -> adapt_content_to_french(content)
      "es" -> adapt_content_to_spanish(content)
      "pt" -> adapt_content_to_portuguese(content)
      "zh" -> adapt_content_to_chinese(content)
      "ja" -> adapt_content_to_japanese(content)
      "ar" -> adapt_content_to_arabic(content)
      "it" -> adapt_content_to_italian(content)
      "ru" -> adapt_content_to_russian(content)
      _ -> content  # Fallback to English
    end
  end

  # French vocabulary translations
  defp adapt_content_to_french(content) do
    adapted_vocabulary = Enum.map(content.vocabulary, fn item ->
      get_french_translation(item)
    end)
    %{content | vocabulary: adapted_vocabulary}
  end

